<% if !@asks.blank? %>
  <% @asks.each do |item| %>
    jQuery(".pagination").before('<%= escape_javascript(render("/asks/ask",:item => item)) %>');
  <% end %>
  current_page = "<%= (params[:page] || "1").to_i + 1 %>";
  <% if @asks.count < @per_page %>
  jQuery(".pagination").remove();
  <% end %>
<% else %>
  jQuery(".pagination").remove();
<% end %>

