<div data-widget-id="2" class="dashboard-widget display analytics yt-uix-dragdrop-draggable-item">
	<%= render partial:'mine/dashboard_widget_header',locals:{title:'analytics',widget:'remove'} %>
	<div class="dashboard-widget-viewport">
		<div class="dashboard-widget-inside clearfix">
			<div class="content">
				<a href="https://www.%E8%AF%BE%E4%BB%B6TV.com/analytics?feature=vmdshb"></a>
				<div class="clearfix section">
					<div class="section-sparkline">
