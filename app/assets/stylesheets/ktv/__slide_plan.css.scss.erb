	}
	.modal_login label {
	  font-weight: 400;
	}
	.modal_login input.text {
	  width: 146px;
	}
	.proLoginTitle {
	  background: url(<%=asset_path('slide/public.slidesharecdn.com/images/business/plans/lock-icon.png')%>) no-repeat;
