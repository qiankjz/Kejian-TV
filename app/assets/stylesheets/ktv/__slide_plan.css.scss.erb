	  margin: 0;
	}
	.modal_signup label {
	  width: 67px;
	}
	li.signupDisclaimer .TOS {
	  font-size: 0.916em;
	  font-weight: 400;
	  text-align: left;
	  width: 197px;
	}
	li.submitFormButton {
	  padding: 0 0 2em;
	}
	li.submitFormButton input {
	  margin-bottom: .5em;
	}
	.ctaNormal {
	  background: #c7c7c7 url(<%=asset_path('slide/public.slidesharecdn.com/images/cta_normal.png')%>) repeat-x;
	  border: 1px solid #888;
	  color: #333;
	  cursor: pointer;
	  font-weight: 700;
	  outline: 0;
	  padding: .5em 1em;
	  text-shadow: #FFF 1px 1px 1px;
	}
