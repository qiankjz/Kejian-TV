	.modal-action-message {
	  float: left;
	  font-size: 0.916em;
	  margin: 0;
	  padding: 0 0 .25em 76px;
	  width: 284px;
	}
	.modal-action-fail {
	  color: #8a1f11;
	}
	.modal-action-success {
	  color: #090;
	}
	.loginError {
	  color: #8a1f11;
