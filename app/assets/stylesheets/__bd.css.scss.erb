  .focus-hook {
      position: relative;
      zoom: 1;
      z-index: 2;
  }

  .focus-hook .focus-cover {
      position: relative;
      border: 1px solid #D7D7D7;
      border-bottom: none;
      overflow: hidden;
  }

  .focus-hook .loading {
      position: absolute;
      text-align: center;
      left: 45%;
      top: 45%;
  }

  .focus-hook a.arrow {
      top: 104px;
      position: absolute;
      height: 34px;
      width: 34px;
      z-index: 20;
      _zoom: 1;
  }

  .focus-hook p.arrowp {
      position: absolute;
      top: 0;
      width: 0;
      height: 100%;
      z-index: 15;
  }

  .focus-hook p.prevp {
      left: 0;
  }

  .focus-hook p.nextp {
      right: 0;
  }

  .focus-hook .prev {
      left: 5px;
      _cursor: pointer;
  }

  .focus-hook .prev:hover {
      text-decoration: none;
      _cursor: pointer;
  }

  .focus-hook .next {
      _cursor: pointer;
  }

  .focus-hook .next:hover {
      text-decoration: none;
      _cursor: pointer;
  }

  .focus-hook .rotate {
      -webkit-transform: rotate(180deg);
      -webkit-transition: all 400 ms ease-in-out;
  }

  .focus-hook .rerotate {
      -webkit-transform: rotate(0deg);
      -webkit-transition: all 400 ms ease-in-out;
  }

  .focus-hook ul {
      position: absolute;
      top: 0;
  }

  .focus-hook ul li {
      position: absolute;
      top: 0;
      display: none;
      z-index: 0;
  }

  .focus-hook ul li.x3d {
      -webkit-animation-name: x;
      -webkit-animation-duration: 1s;
  }

  .focus-hook ul li.re-x3d {
      -webkit-animation-name: return-x;
      -webkit-animation-duration: 1s;
  }

  .focus-hook ul li.y3d {
      -webkit-animation-name: y;
      -webkit-animation-duration: 1s;
  }

  .focus-hook ul li.re-y3d {
      -webkit-animation-name: return-y;
      -webkit-animation-duration: 1s;
  }

  .focus-hook ul li.z3d {
      -webkit-animation-name: z;
      -webkit-animation-duration: 1s;
  }

  .focus-hook ul li.z3dzoom {
      -webkit-animation-name: return-z;
      -webkit-animation-duration: 1s;
  }

