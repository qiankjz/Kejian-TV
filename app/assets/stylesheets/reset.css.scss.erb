.__qa{
  margin:0;
  padding:0;
  div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,header,footer,menu,nav,section,article,aside,figure{margin:0;padding:0}
  section,figure,header,footer,aside,article,hgroup,figcaption,nav,menu{display:block}
  fieldset,img,abbr,acronym{border:none}
  h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:400}
  a{outline:none}
  input{border:none;background:none;}
  button{border:none;vertical-align:middle;cursor:pointer}
  label{cursor:pointer}
  table {border-collapse:collapse;border-spacing:0;}
  address,caption,cite,code,dfn,em,th,var {font-weight:normal;font-style:normal;}
  ol,ul {list-style:none;}
}