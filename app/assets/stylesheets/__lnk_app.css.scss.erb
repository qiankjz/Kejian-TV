.__lnk{
  /* scss/deprecated/applications.css */

  .canvas #content.ad-support {
      width: 780px;
      float: left;
  }

  .canvas #main #extra {
      display: block;
      width: 180px;
      float: right;
      zoom: 1;
  }

  .canvas #main #extra #gadget-logo {
      width: 100%;
      height: 0;
      overflow: hidden;
  }

  .canvas #main #extra .ad-block .content {
      width: auto;
      margin: 0;
  }

  .canvas .alert {
      margin: 15px;
  }

  #content p {
      font-size: 13px;
      line-height: 1.25;
      margin-top: 1.25em;
      position: relative;
      z-index: 1;
  }

  #main p {
      font-size: 13px;
      line-height: 1.25;
  }

  .app-directory h2 {
      border-bottom: 1px solid #ccc;
      font-size: 220%;
      font-weight: bold;
      line-height: 1;
      margin-bottom: 7px;
      padding-bottom: 10px;
      *padding-top: 10px;
      _padding-top: 10px;
  }

  .app-directory .apps {
      margin-top: 30px;
  }

  .app-directory .apps ul {
      clear: both;
      min-height: 126px;
      _height: 126px;
  }

  .app-directory .apps li {
      float: left;
      margin-right: 20px;
      min-height: 190px;
      _height: 190px;
      padding-bottom: 20px;
      padding-left: 140px;
      position: relative;
      width: 258px;
      &:nth-child(even){
        margin-right:0;
      }
  }

  .fr .app-directory .apps li,.de .app-directory .apps li {
      min-height: 220px;
      _height: 220px;
  }

  .app-directory .apps ul li {
      visibility: hidden;
  }

  .app-directory .apps h3 {
      font-size: 180%;
      font-weight: normal;
      line-height: 1.15;
      padding-top: 7px;
  }

  .app-directory .apps p {
      font-size: 13px;
      line-height: 1.25;
  }

  .app-directory .apps p.author {
      color: #666;
      font-size: 12px;
      line-height: 1;
      margin: 2px 0 10px;
  }

  .app-directory .apps .thumbnail {
      height: 126px;
      left: 0;
      position: absolute;
      width: 126px;
      z-index: 3;
  }

  .app-directory .apps .thumbnail a {
      cursor: pointer;
      display: block;
      height: 100%;
      width: 100%;
  }

  .app-directory .apps .thumbnail img {
      display: block;
      margin: 3px;
  }

  .app-directory .apps .thumbnail span {
      background:  url(<%=asset_path('ln02/scds/common/u/img/applications/icon_shadow_126x126.png')%>) no-repeat;
      height: 100%;
      left: 0;
      position: absolute;
      top: 0;
      width: 100%;
      z-index: 3;
  }

  * html .app-directory .apps .thumbnail span {
      background-image: none;
      filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src="/img/applications/icon_shadow_126x126.png", sizingMethod="crop");
  }

  #page-title.sub {
      margin-bottom: 0;
  }

  .app-install #main {
      margin-top: 0;
      overflow: hidden;
      zoom: 1;
  }

  .app-install #extra p {
      font-size: 12px;
  }

  .app-install #content li {
      background:  url(<%=asset_path('ln01/scds/common/u/img/icon/icon_bullet_gray_5x5.gif')%>) no-repeat 0 9px;
      font-size: 13px;
      line-height: 1.25;
      padding: 3px 0 3px 12px;
  }

  .app-install #content ul.legal {
      margin: 10px auto 0;
  }

  .app-install #content ul.legal li {
      background: none;
      border-right: 1px solid #ccc;
      font-size: 12px;
      padding: 0 5px 0 0;
      margin: 0 5px 0 0;
      list-style-type: none;
      float: left;
  }

  .app-install #content ul.legal li.last {
      border-right: none;
  }

  .app-install #extra .reminder {
      padding: 10px 15px;
      margin: 0 0 10px;
      background:  url(<%=asset_path('ln01/scds/common/u/img/bg/bg_yellow_gradient_tip_250x56.png')%>) no-repeat 0 0;
  }

  .app-install #extra .reminder h2 {
      font-size: 13px;
      color: #cc6600;
      margin-bottom: 10px;
  }

  .app-install #extra .reminder li {
      font-size: 13px;
      list-style-type: disc;
      margin-left: 15px;
  }

  .showcase {
      margin-bottom: -54px;
      _margin-bottom: -24px;
      overflow: hidden;
  }

  .showcase h2 {
      background: #e8e8e8  url(<%=asset_path('ln02/scds/common/u/img/bg/bg_e8e8e8_1x25.gif')%>) 0 0 repeat-x;
      border: 1px solid #ddd;
      color: #555;
      font-size: 13px;
      font-weight: bold;
      line-height: 25px;
      padding: 0 0 0 10px;
  }

  .showcase #screenshot {
      border: 1px solid #ddd;
      border-width: 0 1px 1px;
      height: 184px;
      overflow: hidden;
      _width: 505px;
  }

  .showcase #screenshot img {
      display: none;
  }

  .showcase #screenshot img.current {
      display: inline;
  }

  .showcase .footer {
      background:  url(<%=asset_path('ln01/scds/common/u/img/bg/bg_wave_mask_v3.png')%>) 0 0 no-repeat;
      _background: none;
      width: 560px;
      *width: 557px;
      height: 48px;
      position: relative;
      left: 0;
      top: -48px;
      _top: -12px;
  }

  #ctrl {
      display: none;
      position: absolute;
      right: 0;
      bottom: 0;
      _bottom: 2px;
  }

  #ctrl span,#ctrl a {
      float: left;
      text-indent: -12345px;
      height: 19px;
      width: 22px;
  }

  a#ctrl-back {
      background:  url(<%=asset_path('ln01/scds/common/u/img/pic/pic_carousel_sprites.gif')%>) no-repeat 0 0;
      cursor: pointer;
      margin-left: 4px;
  }

  a#ctrl-forward {
      background:  url(<%=asset_path('ln01/scds/common/u/img/pic/pic_carousel_sprites.gif')%>) no-repeat -22px 0;
      width: 23px;
      cursor: pointer;
  }

  #ctrl-back:hover {
      background-position: 0 -39px;
  }

  #ctrl-forward:hover {
      background-position: -22px -39px;
  }

  #ctrl-back.disabled {
      background-position: 0 -78px;
  }

  #ctrl-forward.disabled {
      background-position: -22px -78px;
  }

  #ctrl .counter {
      width: 8px;
      background: #999  url(<%=asset_path('ln01/scds/common/u/img/pic/pic_carousel_sprites.gif')%>) no-repeat -18px -162px;
      margin: 7px 6px 0 0;
  }

  #ctrl .counter.current {
      background-color: #069;
      background-position: -18px -123px;
  }

  .feature {
      background: #fff  url(<%=asset_path('ln01/scds/common/u/img/pic/pic_grey_border_1800x1800.gif')%>) 0 100% no-repeat;
      height: 1%;
      margin: 0;
      padding: 0;
  }

  #developer-logo {
      text-align: center;
      margin-top: .5em;
  }

  .feature h2 {
      background: #3388b1  url(<%=asset_path('ln01/scds/common/u/img/bg/bg_blue_hdr_r_595x27.gif')%>) right top no-repeat;
      cursor: default;
      margin: 0;
      padding: 0;
  }

  .feature h2 span {
      display: block;
      background:  url(<%=asset_path('ln01/scds/common/u/img/bg/bg_blue_hdr_l_5x27.gif')%>) left top no-repeat;
      font-size: 13px;
      color: #fff;
      font-weight: bold;
      padding: 0 0 0 10px;
      line-height: 27px;
  }

  .feature .content {
      border: none;
      display: block;
      background: #fff  url(<%=asset_path('ln01/scds/common/u/img/pic/pic_grey_border_1800x1800.gif')%>) 100% 100% no-repeat;
      margin: 0 0 0 10px;
      padding: 10px 10px 10px 0;
  }

  .feature form {
      padding: 14px 0 0;
      text-align: center;
  }

  .feature fieldset {
      background:  url(<%=asset_path('ln02/scds/common/u/img/bg/bg_grey_dotted_h-line_3x1.png')%>) repeat-x 0 0;
      padding: 14px 0 12px;
  }

  .feature legend {
      text-indent: -12345px;
      margin: -1em;
  }

  .feature form ul {
      margin-bottom: 10px;
  }

  .feature form li {
      text-align: left;
  }

  .feature form li input {
      vertical-align: middle;
  }

  .feature form li label {
      padding-left: .15em;
      line-height: 1;
  }

  .de .feature form .btn-primary,.fr .feature form .btn-primary {
      *width: 200px;
      margin: 0 auto;
  }

  .feature form .btn-second,.feature form .btn-secondary {
      margin-left: 5px;
  }

  .feature dl {
      margin: 14px 0;
      background:  url(<%=asset_path('ln02/scds/common/u/img/bg/bg_grey_dotted_h-line_3x1.png')%>) repeat-x 0 100%;
      padding: 0 0 14px;
  }

  .feature dt,.feature dd,.feature li {
      font-size: 12px;
  }

  .feature dt {
      float: left;
      width: 45%;
      text-align: right;
      font-weight: bold;
      color: #666;
      background-color: #ddf0f8;
      margin-bottom: 1px;
      padding: 4px 6px;
      margin-right: 8px;
  }

  .feature dd {
      margin-bottom: 1px;
      padding: 4px 0;
  }

  .feature p {
      font-size: 12px;
      margin-top: 1em;
      color: #666;
      line-height: 1.25;
  }
  
}