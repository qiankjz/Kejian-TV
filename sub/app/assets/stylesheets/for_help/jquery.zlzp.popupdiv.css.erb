#divPopup {margin:0;padding:0;background:#ffffff;overflow:hidden;}
#popupTit {height:30px;line-height:30px;padding-right:5px; background-color:#990000;color:#FFF2C6;}
#popupTitle {font-size:14px;font-weight:bold;text-align:left;height:30px;line-height:30px;padding-left:40px; background:url(<%=asset_path("for_help/zlzp/layer_title.png")%>) center left no-repeat;overflow:hidden;-moz-user-select:none;}
#popupClose {float:right;margin-top:6px;width:18px;height:18px;overflow:hidden;text-indent:-1000px;background:url(<%=asset_path("for_help/zlzp/layer_close.png")%>) no-repeat;cursor:pointer;}
#popupCon {border-left:2px solid #990000;border-right:2px solid #990000;border-bottom:2px solid #990000;overflow:hidden;}
#popupInnerCon {background:#FFF2C5; overflow:hidden;}
#popupInnerCon .msg{background:url(<%=asset_path("for_help/zlzp/layer_bg.png")%>) no-repeat top;text-align:center;padding:30px 10px 10px;font-size:14px;font-weight:bold;color:#990000;}
#popupInnerCon .ajaxLoading{background:#FFF2C5;text-align:center;padding:50px;font-size:14px;color:#990000;}
#popupInnerCon .ajaxError{background:#FFF2C5;text-align:center;padding:59px;font-size:14px;color:#990000;}
#popupInnerCon .buttons {background:#FFF2C5;text-align:center;padding:10px 10px 30px;}
#popupInnerCon .buttons span {display:inline-block;padding:0;border:0;width:103px;height:41px;line-height:41px;font-size:14px;font-weight:bold;cursor:pointer;}
#popupInnerCon .buttons span.popupConfirmBtn {color:#FEF4C3; background:url(<%=asset_path("for_help/zlzp/layer_button.png")%>) no-repeat;}
#popupInnerCon .buttons span.popupConfirmBtn:hover, #popupInnerCon .buttons span.popupConfirmBtnHover {color:#FEF4C3; background:url(<%=asset_path("for_help/zlzp/layer_button.png")%>) no-repeat;}
#popupInnerCon .buttons span.popupCancelBtn {margin-left:20px;color:#3359aa;background:url(<%=asset_path("for_help/zlzp/new_v5/popup_bg.png")%>) no-repeat 0 -58px;}
#popupInnerCon .buttons span.popupCancelBtn:hover, #popupInnerCon .buttons span.popupCancelBtnHover {margin-left:20px;color:#3359aa;background:url(<%=asset_path("for_help/zlzp/new_v5/popup_bg.png")%>) no-repeat 0 -29px;}

#popupInnerCon .msg span{ font-weight:normal;}
.imgs{ width:187px; height:130px; margin:auto;}
.imgs img{ border:1px solid #DC9E55}
.email_all{ width:309px; height:32px; margin:auto; line-height:32px;}
.email_name{ width:61px; height:32px; color:#990000; text-align:right; margin-right:10px; float:left;}
.email_input{ width:238px; height:32px; float:left;}
.email_input input{ width:236px; height:30px; font-size:14px; color:#555; border:1px solid #DC9E55;}
.email_button{ width:123px; height:41px; line-height:41px; color:#FFF2C5; background:url(<%=asset_path("for_help/zlzp/email_button.png")%>) no-repeat; margin-left:auto; margin-right:auto; margin-top:10px; cursor:pointer;}