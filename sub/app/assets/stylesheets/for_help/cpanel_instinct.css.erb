/* 翻页 start */
.backpager{
background:#f7f7f7;
margin:10px 0;
float:right;
border:1px solid #cad9ea; 
}
.backpager ul{
margin:0px;
padding:0px;
}
.backpager li a{
color:black;
font-family:arial,simsun;
}
.backpager li{
float:left;
cursor:pointer;
font-size:12px;
list-style:none;
}
.backpager li span{
padding:4px 5px 2px;
float:left;
}
.backpager li.more{
cursor:auto;
}
.backpager li.current span, .backpager li .current{
background:#cad9ea;
}
.backpager li.current a, .backpager li .current a{
color:#00a483;
}
.backpager span input{
width:50px;
height:15px;
padding:0px;
}
/* 翻页 end */

/* 管理侧边栏 start */
#cpanel_sidebar{
float:left;
width:130px;
margin-right:40px;
margin-left:20px;
}
#cpanel_sidebar .side_table{
width:130px;margin-right:20px;float:left;border:1px solid #888;margin-bottom:30px;
}
#cpanel_sidebar .side_th{
background:#006600;
color:white;
width:130px;
height:20px;
line-height:20px;
margin-bottom:4px;
font-family:微软雅黑;
font-size:13px;
font-weight:normal;
}
#cpanel_sidebar .side_td{
width:130px;
height:20px;
margin-bottom:8px;
}
#cpanel_sidebar .side_td a{
font-family:微软雅黑;
font-size:13px;
font-weight:normal;
text-decoration:none;
color:#333333;
}
/* 管理侧边栏 end */

/* header start */
#header #app_nom{
margin-left:20px;
float:left;
font-family:黑体;
font-size:16px;
font-weight:bold;
}
#header .app_admin{
float:right;
font-family:微软雅黑;
font-size:14px;
}
/* header start */
/* 前台公告显示 start */
#fore_sys{
font:normal 14px/22px "Hiragino Sans GB",simsun;
color:#555;
}
#fore_sys div{
margin:0;
padding:0;
display:block;
}
#fore_sys #fore_sys_notice{
width:606px;
min-height:44px;
margin:10px auto 20px;;
background-color:#FFFBC3;
padding:15px;
border:2px solid #FFC30B;
font-size:12px;
color:#999;
position:relative;
}
#fore_sys .fore_gray_button{
color:#369;
}
#fore_sys #fore_sys_notice .info{
margin-top:0px;
}
/* 前台公告显示 end */
/* 后台用户编辑 start */
#cpanel_edit_user .inputs,#cpanel_ask .inputs,#cpanel_answer .inputs,#cpanel_comment .inputs,#cpanel_topic .inputs{
margin-left:35px;
margin-top:10px;
}
#cpanel_edit_user .inputs input,#cpanel_ask .inputs input,#cpanel_answer .inputs input,#cpanel_comment .inputs input,#cpanel_topic .inputs input{
width:13px;
margin-right:0px;
}
#cpanel_edit_user .actions,#cpanel_ask .actions,#cpanel_answer .actions,#cpanel_comment .actions,#cpanel_topic .actions{
margin-left:45px;
margin-top:30px;
margin-bottom:40px;
}
/* 后台用户编辑 end */
/* 后台用户权限 start */
#edit_admin_form{
width:790px;
height:460px;
margin-top:20px;
}
#edit_admin_form h3{
margin-left:4px;
}
#edit_admin_form h3 span{
font-size:13px;
font-weight:200;
color:#390;
margin-left:10px;
}
#edit_admin_form .sup_check{
width:13px;
margin-left:3px;
}
#edit_admin_form .sub_check{
width:13px;
margin-left:70px;
}
#edit_admin_form #admin_name{
margin-left:2px;
margin-top:1px;
margin-bottom:30px;
}
/* 后台用户权限 end */
/* 后台登录页面 start */
#login_page{
width:490px;
height:170px;
margin:50px auto;
border:1px solid #797979;
}
#login_page .login_div{
width:250px;
height:25px;
margin:0px auto;
margin-top:20px;
}
#login_page .input_div{
font-size:13px;
float:left;
line-height:25px;
}
#login_page input{
width:150px;
margin-left:20px;
}
/* 后台登录页面 end */
/* 后台欢迎界面 start */
#welcome_div{
width:770px;
height:250px;
height:150px!important;
padding-top:100px;
color:#060;
border:1px solid #060;
margin:50px auto;
font-weight:800;
font-size:18px;
text-align:center;
}
/* 后台欢迎界面 end */
/* 公共样式 start */
/*绿色的行*/
body{
min-width:1160px;
}
.green_header{
background:#060;
color:white;
height:20px;
line-height:20px;
font-family:微软雅黑;
font-size:13px;
}
/*表格*/
.stat_table{
width:100%;
border:1px solid #797979;
border-width:0 0 1px 1px;
border-collapse:collapse;
border-spacing:0px;
}
.stat_table td{
padding:8px 0px;
}
.stat_table2 td{
padding:0px;
}
.stat_table .add_padding{
padding:8px;
line-height:2em;
}
.stat_table tr.odd{
background-color:white;
}
.stat_table tr.even{
background-color:#e8edff;
}
.stat_table tr:hover td{
background-color:#ffffaa;
cursor:default;
}
.stat_table th,.stat_table td{
color:#333;
height:24px;
padding-left:2px;
border:1px solid #797979;
border-width:1px 1px 0 0;
}
.stat_table td a{
color:#333;
}
.stat_table #check_btn{
width:50px;
margin-right:3px;
cursor:pointer;
}
/*富文本编辑器*/
.kedit_form{
padding:10px 110px;
width:738px!important;
width:958px;
margin-bottom:20px;
border:1px solid #888;
padding-top:10px;
}
/*搜索表格*/
.search_form{
width:958px;
margin-bottom:20px;
border:1px solid #888;
padding-top:10px;
}
.search_form .search_form_row{
margin-left:10px;
margin-bottom:10px;
}
.search_form .search_form_row label{
margin-right:5px;
}
.search_form .search_form_row input{
margin-right:15px;
}
.search_form .search_form_row select{
margin-right:80px;
}
.search_form .search_form_row .left_count{
width:50px;
margin-right:0px;
}
.search_form .search_form_row .right_count{
width:50px;
margin-right:30px;
}
/* 自动换行 */
.wordwrap
{
word-wrap:break-word;
word-break:break-all;
-moz-binding: url('./wordwrap.xml#wordwrap');
overflow: hidden;
}
