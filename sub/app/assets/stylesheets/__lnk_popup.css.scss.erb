    float: right;
    margin: 4.5px 5px 0 0;
}

#header .meta .close {
    float: right;
    display: block;
    margin: 4px 0 0 10px;
    background: url(<%=asset_path('ln01/scds/common/u/img/sprite/sprite_global_v6.png')%>) 100% -1300px no-repeat;
    width: 17px;
    height: 14px;
    padding: 0;
    text-indent: -123456px;
    cursor: pointer;
}

#footer .wrapper {
    min-width: 710px;
    max-width: 98em;
    _width: 98em;
    margin: 0 auto;
}

#footer a {
    color: #069;
}

#footer p {
    font-size: 12px;
    margin: 0;
    padding: 0;
}

#popmain {
    text-align: left;
    padding: 5px 10px;
}

.plaintext h1,.pagetitle {
    font-size: 20px;
    margin-bottom: 15px;
}

.plaintext h2 {
    font-size: 17px;
    margin-bottom: 15px;
}

.plaintext h3 {
    font-size: 15px;
    margin-bottom: 15px;
}

.plaintext h4 {
    font-size: 13px;
    margin-bottom: 15px;
    font-weight: bold;
}

.plaintext h1 a,.plaintext h2 a,.plaintext h3 a,.plaintext h4 a {
    color: #000;
    text-decoration: none;
}

.plaintext p {
    font-size: 13px;
    margin-bottom: 15px;
}

.plaintext ul {
    margin-bottom: 15px;
    margin-left: 30px;
    list-style: disc;
}

.plaintext ol {
    margin-bottom: 15px;
    margin-left: 30px;
    list-style: decimal;
}

.plaintext li {
    font-size: 13px;
}

.plaintext li li {
    font-size: 12px;
}

.plaintext li ul,.plaintext li ol {
    margin-bottom: 0;
}

.plaintext dt {
    font-size: 13px;
    font-weight: bold;
    margin-bottom: 15px;
}

.plaintext .helper {
    font-size: 92%;
    margin-bottom: 0;
}

.plaintext .btn {
    text-align: center;
}

.plaintext em {
    font-style: italic;
}

.plaintext .last-changed {
    color: #999;
    font-size: 12px;
    font-style: italic;
    text-align: right;
    margin-bottom: 0;
}

.legal h2 {
    font-size: 150%;
}

.legal h4 {
    font-size: 13px;
    font-style: italic;
    font-weight: normal;
    margin-bottom: 0;
}

.legal h4 {
    font-style: italic;
}

.legal li {
    margin-bottom: 1em;
}

.legal li p,.legal li h2,.legal li h3,.legal li h4,.legal li li {
    font-size: 12px;
}

.legal li ul {
    list-style: circle;
    margin: 1em 0 1em 1em;
    padding-left: 1em;
}

.legal .section-start {
    border-top: 1px solid #ccc;
    padding-top: 15px;
}