    font-size: 85%;
    height: 26px;
    margin: 0 0 0 17px;
    padding: 0;
    list-style: none;
    white-space: nowrap;
    overflow: hidden;
}

.tabs li,.tabs li a,.tabs li strong {
    display: block;
    float: left;
}

.tabs li {
    background: url(<%=asset_path('ln02/scds/common/u/img/bg/bg_sprites_tabs.png')%>) 0 -47px no-repeat;
    margin: 0;
    padding: 0;
}

.tabs li.selected {
