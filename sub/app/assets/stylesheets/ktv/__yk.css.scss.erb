.__yk{

  /*element*/
  html{direction:ltr;*overflow-y:scroll;}
  height:100%;
  padding:0;margin:0;font:12px/20px arial, helvetica, verdana, tahoma, sans-serif;word-break:break-all;word-wrap:break-word;  div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,button,select,option,textarea,p,blockquote,th,td{padding:0;margin:0;font:12px/20px arial, helvetica, verdana, tahoma, sans-serif;word-break:break-all;word-wrap:break-word;}
  img{border:0;vertical-align:top;}
  ol,ul,dl{list-style:none;}
  h1{font:bold 16px/18px "microsoft yahei","微软雅黑";}
  h2{font-weight:bold;font-size:16px;}
  h3{font-weight:bold;font-size:14px;}
  h4{font-weight:bold;font-size:12px;}
  h5{font-weight:bold;font-size:12px;}
  h5{font-weight:bold;font-size:12px;}
  h6{font-weight:bold;font-size:12px;}
  a{cursor:pointer;color:#014ccc;text-decoration:none;}
  a:hover{text-decoration:underline;}
  select{height:20px;vertical-align:middle;}
  option{height:20px;line-height:20px;padding-right:6px;}
  textarea{resize:vertical;}
  input{vertical-align:middle;display:inline-block;*display:inline;*zoom:1;height:16px;line-height:16px;_height:auto;_line-height:15px;}
  input[type="file"]{height:22px;line-height:20px;}
  input[type="radio"],input[type="checkbox"]{padding:1px;margin:-1px 0;}
  input[type="button"],input[type="submit"],input[type="reset"]{width:auto;height:22px;line-height:20px;line-height:16px\9;padding:0 10px;*padding:0;*width:1;}
  input[type="search"]{-webkit-appearance:textfield;}
  button,input.btnNormal{vertical-align:middle;height:22px;line-height:18px;padding:0 10px;_line-height:14px;_padding:0;}
  @-moz-document url-prefix(){/*for firefox*/input{height:17px;padding-bottom:1px;}}

  /*style*/
  .modTitle{height:25px;font:bold 16px/18px "microsoft yahei","微软雅黑";overflow:hidden;}
  .bigTitle{font:bold 16px/18px "microsoft yahei","微软雅黑";}
  .title{font-size:14px;font-weight:bold;}
  .title a{font-size:14px;font-weight:bold;color:#000;}
  .title a:hover{text-decoration:underline;}
  a.bLink{display:block;border:1px solid #bbdded;height:26px;line-height:26px;}
  a.bLink:hover{background:#bbdded;}
  a.videoClass{color:#276d17;}
  .grayLink a{color:#909090;}
  .grayLink a:hover{color:#014ccc;}
  /*style.text*/
  .arrow{font-family:simsun,"\5B8B\4F53","宋体";}
  .num{font-size:10px;}
  .boldNum,.boldnum{font-size:11px;font-weight:bold;}
  .disabled{color:#909090;}

  .likelink{cursor:pointer;color:#014ccc;}
  .likelink:hover{text-decoration:underline;}
  .break{font-style:normal;color:#909090;margin:0 4px;}
  .underline{text-decoration:underline;}
  .linethrough{text-decoration:line-through;}
  .overline{text-decoration:overline;}
  .bold{font-weight:bold;}
  .italic{font-style:italic;}
  .sub{vertical-align:sub;}
  .sup{vertical-align:super;}
  .italic{font-style:italic;}
  .f_10{font-size:10px;}
  .f_12{font-size:12px;}
  .f_14{font-size:14px;}
  .f_16{font-size:16px;}
  .f_18{font-size:18px;}
  .f_20{font-size:20px;}
  .t_c{text-align:center;}
  .t_l{text-align:left;}
  .t_r{text-align:right;}
  .t_m{vertical-align:middle;}
  .t_t{vertical-align:text-top;}
  .t_b{vertical-align:text-bottom;}
  .c_gray{color:#909090;}
  .c_red{color:#f00;}
  .c_orange{color:#ffa500;} 
  .c_yellow{color:#ff0;}
  .c_green{color:#008000;}
  .c_aqua{color:#0ff;}
  .c_blue{color:#00f;}
  .c_violet{color:#ee82ee;}
  /*style.effect*/
  .opacity10{filter:alpha(opacity=10);opacity:0.1;}
  .opacity20{filter:alpha(opacity=20);opacity:0.2;}
  .opacity30{filter:alpha(opacity=30);opacity:0.3;}
  .opacity40{filter:alpha(opacity=40);opacity:0.4;}
  .opacity50{filter:alpha(opacity=50);opacity:0.5;}
  .opacity60{filter:alpha(opacity=60);opacity:0.6;}
  .opacity70{filter:alpha(opacity=70);opacity:0.7;}
  .opacity80{filter:alpha(opacity=80);opacity:0.8;}
  .opacity90{filter:alpha(opacity=90);opacity:0.9;}
  .boxround5{-moz-border-radius:5px;border-radius:5px;}
  .boxround10{-moz-border-radius:10px;border-radius:10px;}
  .boxround15{-moz-border-radius:15px;border-radius:15px;}
  .boxround20{-moz-border-radius:20px;border-radius:20px;}
  .boxshadow5{-moz-box-shadow:2px 2px 5px #4d4d4d;-webkit-box-shadow:2px 2px 5px #4d4d4d;box-shadow:2px 2px 5px #4d4d4d;}
  .boxshadow10{-moz-box-shadow:3px 3px 10px #4d4d4d;-webkit-box-shadow:3px 3px 10px #4d4d4d;box-shadow:3px 3px 10px #4d4d4d;}
  .boxshadow15{-moz-box-shadow:4px 4px 15px #4d4d4d;-webkit-box-shadow:4px 4px 15px #4d4d4d;box-shadow:4px 4px 15px #4d4d4d;}
  .boxshadow20{-moz-box-shadow:6px 6px 20px #4d4d4d;-webkit-box-shadow:6px 6px 20px #4d4d4d;box-shadow:6px 6px 20px #4d4d4d;}
  .boxshadow5outer{-moz-box-shadow:0 0 5px #4d4d4d;-webkit-box-shadow:0 0 5px #4d4d4d;box-shadow:0 0 5px #4d4d4d;}
  .boxshadow10outer{-moz-box-shadow:0 0 10px #4d4d4d;-webkit-box-shadow:0 0 10px #4d4d4d;box-shadow:0 0 10px #4d4d4d;}
  .boxshadow15outer{-moz-box-shadow:0 0 15px #4d4d4d;-webkit-box-shadow:0 0 15px #4d4d4d;box-shadow:0 0 15px #4d4d4d;}
  .boxshadow20outer{-moz-box-shadow:0 0 20px #4d4d4d;-webkit-box-shadow:0 0 20px #4d4d4d;box-shadow:0 0 20px #4d4d4d;}
  .boxshadow5inner{-moz-box-shadow:inset 1px 1px 5px #4d4d4d;-webkit-box-shadow:inset 1px 1px 5px #4d4d4d;box-shadow:inset 1px 1px 5px #4d4d4d;}
  .boxshadow10inner{-moz-box-shadow:inset 2px 2px 10px #4d4d4d;-webkit-box-shadow:inset 2px 2px 10px #4d4d4d;box-shadow:inset 2px 2px 10px #4d4d4d;}
  .boxshadow15inner{-moz-box-shadow:inset 4px 4px 15px #4d4d4d;-webkit-box-shadow:inset 4px 4px 15px #4d4d4d;box-shadow:inset 4px 4px 15px #4d4d4d;}
  .boxshadow20inner{-moz-box-shadow:inset 6px 6px 20px #4d4d4d;-webkit-box-shadow:inset 6px 6px 20px #4d4d4d;box-shadow:inset 6px 6px 20px #4d4d4d;}
  .rotate45{-moz-transform:rotate(45deg);-o-transform:rotate(45deg);-webkit-transform:rotate(45deg);transform:rotate(45deg);}
  .rotate90{-moz-transform:rotate(90deg);-o-transform:rotate(90deg);-webkit-transform:rotate(90deg);transform:rotate(90deg);}
  .rotate135{-moz-transform:rotate(135deg);-o-transform:rotate(135deg);-webkit-transform:rotate(135deg);transform:rotate(135deg);}
  .rotate180{-moz-transform:rotate(180deg);-o-transform:rotate(180deg);-webkit-transform:rotate(180deg);transform:rotate(180deg);}
  .rotate225{-moz-transform:rotate(225deg);-o-transform:rotate(225deg);-webkit-transform:rotate(225deg);transform:rotate(225deg);}
  .rotate270{-moz-transform:rotate(270deg);-o-transform:rotate(270deg);-webkit-transform:rotate(270deg);transform:rotate(270deg);}
  .rotate315{-moz-transform:rotate(315deg);-o-transform:rotate(315deg);-webkit-transform:rotate(315deg);transform:rotate(315deg);}

  /*style.hack*/
  .nounderline a{text-decoration:none;}
  .noborder{border:none;}
  /*style.*/
  /*vmarker*/
  .vMarker{position:relative;top:0;left:0;height:22px;padding:0 5px;overflow:hidden;background:#fffcc9;border:1px solid #ffd667;margin-bottom:10px;}
  .vMarker .close{position:absolute;top:5px;right:5px;width:10px;height:10px;overflow:hidden;border:1px solid #909090;cursor:pointer;text-align:center;text-indent:-1px\9;font-size:10px;line-height:10px;font-family:verdana;color:#909090;}
  .vMarker .item{margin-right:4px;}
  .vMarker .cop{font-weight:bold;color:#014ccc;line-height:22px;text-decoration:underline;}
  /*vrstat*/
  .VR_stat{background:url(<%=asset_path('yk/index/img/master.png')%>) no-repeat -256px -864px;width:60px;height:15px;overflow:hidden;display:inline-block;*display:inline;*zoom:1; vertical-align:text-bottom;}
  .VR_stat a{display:block;width:100%;height:100%;color:#909090;}
  .VR_stat .num{font-family:arial;position:absolute;margin-left:15px;color:#909090;line-height:14px;font-size:10px;font-weight:normal;width:42px;text-align:center;}
  .VR_stat a:hover .num{text-decoration:underline;}
  /*rating*/
  .rating{display:inline-block;*display:inline;*zoom:1;white-space:nowrap;}
  .rating em{margin-right:3px;}
  .rating .num{font-style:normal;font-weight:bold;font-size:14px;vertical-align:middle;margin-left:2px;color:#909090}

  .userHead{width:30px;height:30px;padding:1px;background:#fff;border:1px solid #e5e5e5;}
  .tilteBar{clear:both;margin-bottom:10px;}
  .userInput,.dataControl{background:#e8f6ff;}
  /*style.lineborder*/
  .line_dot,.linedot,.hr{background:url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -736px;height:1px;overflow:hidden;}

  /*sysinfo*/
  .i_info{}
  .i_notice{}
  .i_pass{}
  .i_error{}
  .i_success{}
  .i_failed{}

  /*layout*/
  .window,.screen{min-width:960px;}
  .s_header,.s_miniHeader,.s_footer,.s_miniFooter,.s_body{clear:both;min-width:960px;}
  .s_main{width:930px;padding:15px;margin:0 auto;}

  .col2_21 .left{float:left;width:610px;}
  .col2_21 .right{float:right;width:300px;}
  .col2_12 .left{float:left;width:300px;}
  .col2_12 .right{float:right;width:610px;}
  .col2_21_p10 .left{float:left;width:610px;}
  .col2_21_p10 .right{float:right;width:280px;padding:10px;}
  .col2_16 .left{float:left;width:120px;}
  .col2_16 .right{float:right;width:790px;}
  .col2_61 .left{float:left;width:790px;}
  .col2_61 .right{float:right;width:120px;}
  .col3_285 .left{float:left;width:120px;margin-right:15px;}
  .col3_285 .center{float:left;width:480px;}
  .col3_285 .right{float:right;width:300px;}
  .col3_852 .left{float:left;width:480px;margin-right:15px;}
  .col3_852 .center{float:left;width:300px;}
  .col3_852 .right{float:right;width:120px;}
  .col3_2112 .left{float:left;width:120px;margin-right:15px;}
  .col3_2112 .center{float:left;width:660px;}
  .col3_2112 .right{float:right;width:120px;}

  .layout_121 .mainCol{float:left;width:610px;}
  .layout_121 .sideCol{float:right;width:300px;}
  .layout_112 .mainCol{float:right;width:610px;}
  .layout_112 .sideCol{float:left;width:300px;}
  .layout_100 .mainCol{width:auto;}
  .layout_100 .sideCol{display:none;}

  .f_l {float:left;}
  .f_r {float:right;}
  .f_c {clear:both;}
  .clear{clear:both;height:0px;overflow:hidden;}
  .clearfix:after{clear:both;content:"";display:block;}.clearfix{*zoom:1;}

  .mod_b{margin-bottom:25px;}
  .mod{margin-bottom:15px;}
  .mod_s{margin-bottom:10px;}

  /*container*/
  .box_title{clear:both;margin-bottom:10px;}
  .box_border{padding:9px;border:1px solid #e5e5e5;height:100%;}
  .box_border1{padding:9px;border:1px solid #a2d2f0;height:100%;}
  .box_border2{padding:8px;border:2px solid #ffef97;height:100%;}
  .box_bg{padding:10px;background:#e5e5e5;}
  .box_bg2{padding:10px;background:#e8f6ff;}
  .box_border_bg{padding:9px;border:1px solid #a2d2f0;background:#e8f6ff;}

  .box{border:1px solid #e5e5e5;background:#fff;height:100%;margin-bottom:15px;}
  .box .head{height:20px;padding:4px 14px;}
  .box .head_atuo{height:auto;padding:14px;}
  .box .head .status{float:left;width:12px;height:12px;_overflow:hidden;margin-right:5px;margin-top:3px;}
  .box .head .status div{vertical-align:top;}
  .box .head .caption{float:left;margin-right:5px;}
  .box .head .caption .title{float:left;}
  .box .head .caption .title em{font-style:normal;margin:0 0 0 -2px;_margin:0 -1px 0 -5px;color:#909090;}
  .box .head .append{float:left;color:#909090;}
  .box .head .append *{color:#909090;}
  .box .head .extend{float:right;}
  .box .head .extend em{font-style:normal;color:#909090;margin:0 4px;}
  .box .neck{position:relative;height:0;line-height:0;font-size:0;}
  .box .body{padding:14px;}

  .mBox{border:1px solid #e5e5e5;background:transparent;}
  .mBox .head{height:auto;padding:0;}
  .mBox .body{padding:0;}
  .mBox .l{padding-left:14px;}
  .mBox .r{padding-right:14px;}
  .mBox .head .c{padding:0;height:20px;}
  .mBox .body .c{padding:14px 0;}
  .mBox .foot{height:0;overflow:hidden;}
  .mBox .foot .c{overflow:hidden;}

  .nBox{border:none;}
  .nBox .head{padding:5px 14px;background:#e6e6e6 url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -1568px;border:1px solid #c6c6c6;}
  .nBox .head_auto{background:#e6e6e6 url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -1568px;border:1px solid #c6c6c6;}
  .nBox .body{border:1px solid #e5e5e5;border-top:none;height:100%;}

  .qBox{border:none;}
  .qBox .head{padding:5px 14px;background:#eaf9fe url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -1120px;border:1px solid #a7d6e0;}
  .qBox .head_auto{background:#d8f4fe url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -1120px;border:1px solid #a7d6e0;}
  .qBox .head .caption .title a{color:#014ccc;}
  .qBox .body{border:1px solid #e5e5e5;border-top:none;height:100%;}

  /*control*/
  .tab_outer{position:relative;}
  .tab_outer .tabs{position:absolute;top:-45px;left:-14px;height:31px;overflow:hidden;}
  .tab_outer li{cursor:pointer;float:left;height:31px;overflow:hidden;border:1px solid #e5e5e5;border-top:none;border-bottom:none;margin-left:-1px;}
  .tab_outer li a,
  .tab_outer li span{overflow:hidden;float:left;display:block;height:31px;padding:0 14px;text-align:center;line-height:30px;*line-height:32px;line-height:32px\9;font-size:14px;}
  .tab_outer li.current{cursor:default;font-weight:bold;background:#fff;}
  .tab_inner{position:relative;height:25px;border-bottom:1px solid #e5e5e5;margin:-14px -14px 14px -14px;}
  .tab_inner .tabs{position:absolute;top:0;overflow:hidden;}
  .tab_inner li{cursor:pointer;float:left;height:26px;overflow:hidden;border:1px solid #e5e5e5;border-top:none;border-bottom:none;margin-left:-1px;}
  .tab_inner li a,
  .tab_inner li span{overflow:hidden;float:left;display:block;height:25px;padding:0 15px;text-align:center;line-height:24px;*line-height:26px;line-height:26px\9;font-size:12px;}
  .tab_inner li.current{cursor:default;font-weight:bold;background:#fff;}

  .right .box .head,.sideCol .box .head{padding-left:9px;padding-right:9px;}
  .right .box .head_auto,.sideCol .box .head_auto{padding:9px;}
  .right .box .body,.sideCol .box .body{padding:9px;}

  .right .mBox .l,.sideCol .mBox .l{padding-left:9px}
  .right .mBox .r,.sideCol .mBox .r{padding-right:9px}
  .right .mBox .head,.sideCol .mBox .head{padding:0;}
  .right .mBox .body,.sideCol .mBox .body{padding:0;}
  .right .mBox .body .c,.sideCol .mBox .body .c{padding:9px 0;}

  .left .qBox .head,.mainCol .qBox .head{background:#eaf9fe none;border-color:#e5e5e5;}
  .left .nBox .head,.mainCol .nBox .head{background:#f2f2f2 none;border-color:#e5e5e5;}

  .qBox .tab_outer li{border-color:#a7d6e0;}
  .left .qBox .tab_outer li, .mainCol .qBox .tab_outer li{border-color:#e5e5e5;}
  .nBox .tab_outer li{border-color:#c6c6c6;}
  .left .nBox .tab_outer li, .mainCol .nBox .tab_outer li{border-color:#e5e5e5;}

  .right .tab_outer .tabs{top:-40px;left:-9px;}
  .right .tab_inner{margin:-9px -9px 9px -9px;}
  .right .tab_aver .tabs{width:298px;overflow:hidden;}
  .right .tab_aver ul{width:300px;}
  .right .tab_aver2 li a,
  .right .tab_aver2 li span{padding:0;width:149px;}
  .right .tab_aver3 li a,
  .right .tab_aver3 li span{padding:0;width:99px;}
  .right .tab_aver4 li a,
  .right .tab_aver4 li span{padding:0;width:74px;}
  .right .tab_aver5 li a,
  .right .tab_aver5 li span{padding:0;width:59px;}
  .right .tab_aver6 li a,
  .right .tab_aver6 li span{padding:0;width:49px;}

  /*control*/
  .nTab{margin-bottom:15px;}
  .nTab .head{height:28px;border:1px solid #c6c6c6;background:#efefef url(<%=asset_path('yk/index/img/master.png')%>) repeat-x 0 -1056px;}
  .nTab .head .tabs{float:left;}
  .nTab .head .tabs li{position:relative;top:0;left:0;float:left;width:80px;height:28px;border-right:1px solid #c6c6c6;}
  .nTab .head .tabs li .label{position:absolute;top:0;left:0;display:block;width:100%;height:16px;padding:6px 0;overflow:hidden;line-height:16px;text-align:center;font-size:14px;font-weight:bold; white-space:nowrap;}
  .nTab .head .tabs  a{position:absolute;top:0;left:0;display:block;width:100%;height:28px;}
  .nTab .head .tabs  a:hover{background:#dee7ed;}
  .nTab .head .tabs .current a{text-decoration:none;cursor:default;outline:0;}
  .nTab .head .tabs .current .label{background:#dee7ed;height:17px;color:#000;cursor:default;}
  .nTab .head .extend{float:right;line-height:18px;padding:4px 5px 0 0;*padding-top:3px;_overflow:hidden;}
  .nTab .head .extend select{height:20px;vertical-align:middle;}
  .nTab .head .extend option{padding-right:6px;}
  .nTab .head .extend .label{color:#909090;}
  .nTab .body{border:5px solid #dee7ed;padding:10px;height:100%;}

  .pageBar{clear:both;color:#909090;height:20px;}
  .pageBar .num{font-size:10px;color:#909090;}
  .pageBar .page{text-align:right;color:#909090;float:right;}
  .pageBar .page .disabled{text-decoration:none;}
  .pageBar .page a{margin:0 2px;}
  .pageBar .page .current{color:#000;font-weight:bold;text-decoration:none;}

  .LPageBar,.LPageBar *{font-size:14px;line-height:24px;text-align:center;}
  .LPageBar .page a{margin:0 3px;padding:1px 6px 3px 6px;border:1px solid #bbdded;}
  .LPageBar .page a:hover{background:#bbdded;}
  .LPageBar .page .current{font-weight:bold;margin:0 5px;}

  .qPager{clear:both;height:22px;_overflow:hidden;margin-top:10px;}
  .qPager .stat{float:left;font-size:12px;color:#909090;margin-right:10px;line-height:22px;}
  .qPager .pages{float:right;}
  .qPager .pages li{float:left;height:22px;margin-right:3px;}
  .qPager .pages li a,.qPager .pages li span{float:left;display:block;height:20px;padding:0 3px 0 2px;line-height:22px;font-size:14px;overflow:hidden;}
  .qPager .pages li a:hover{height:20px;line-height:20px;border:1px solid #c6c6c6;padding:0 2px 0 1px;text-decoration:none;}
  .qPager .pages li.current span{height:22px;background:#c6c6c6;color:#fff;font-weight:bold;cursor:default;}
  .qPager .pages li.pass{color:#909090;font-weight:bold;line-height:16px;}
  .qPager .turn{float:right;width:125px;}
  .qPager .turn li{height:22px;float:left;}
  .qPager .turn .ico_pre{display:block;position:absolute;top:2px;left:2px;width:16px;height:16px;background:url(<%=asset_path('yk/index/img/master.png')%>) no-repeat -188px -190px;}
  .qPager .turn .ico_next{display:block;position:absolute;top:2px;right:2px;width:16px;height:16px;background:url(<%=asset_path('yk/index/img/master.png')%>) no-repeat -218px -190px;}
  .qPager .turn a, .qPager .turn span{display:block;position:relative;width:48px;height:20px;line-height:20px;*line-height:22px;text-align:center;border:1px solid #e5e5e5;_overflow:hidden;_zoom:1;}
  .qPager .turn a:hover{border-color:#c6c6c6;text-decoration:none;}
  .qPager .turn span{color:#c6c6c6;cursor:default;}
  .qPager .turn span .ico_pre{background:url(<%=asset_path('yk/index/img/master.png')%>) no-repeat -252px -190px;}
  .qPager .turn span .ico_next{background:url(<%=asset_path('yk/index/img/master.png')%>) no-repeat -282px -190px;}
  .qPager .turn .pre{float:left;}
  .qPager .turn .next{float:left;margin-left:5px;}
  .qPager .turn .pre a, .qPager .turn .pre span{padding-left:10px;}
  .qPager .turn .next a, .qPager .turn .next span{padding-right:10px;}

  .qPager_large{height:24px;}
  .qPager_large .stat{display:none;}
  .qPager_large .pages{float:left;}
  .qPager_large .pages li{height:24px;margin-right:4px;}
  .qPager_large .pages li a,.qPager_large .pages li span{height:22px;line-height:24px;font-size:16px;}
