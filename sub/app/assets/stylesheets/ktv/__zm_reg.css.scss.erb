body,.w460,#main{
  background:#52C4B2 !important;
}
.__zm{
html {
    background: #64c5b2;
}

    padding: 0;

div {
    margin: 0 auto;
}

#reg-header {
    width: 625px;
    height: 88px;
    padding-top: 42px;
    text-align: center;
    background: #52C4B2 url(http://a.imimg.cn/r17259/images/reg/headbg.png) no-repeat left bottom;
}

#content {
    width: 625px;
}

.newreg,.newreg2 {
    background: #fff url(http://a.imimg.cn/r17259/images/reg/bottom.png) no-repeat left bottom;
    padding-bottom: 16px;
}

.platformusers h2 {
    text-align: center;
    font-size: 18px;
    height: 54px;
    padding-top: 14px;
}

.platformusers ul {
    width: 360px;
    margin: 0 auto;
    display: block;
}

.platformusers li {
    float: left;
    margin: 0 10px 10px;
    _display: inline;
}

.platformusers li a {
    width: 135px;
    height: 38px;
    padding-left: 25px;
    line-height: 38px;
    color: #666;
    font-size: 16px;
    display: block;
    background: url(http://a.imimg.cn/r17259/images/array/connect_bg.gif) no-repeat left top;
}

.platformusers li a:hover {
    background: url(http://a.imimg.cn/r17259/images/array/connect_bg.gif) no-repeat left -39px;
    text-decoration: none;
}

.platformusers li a em {
    display: inline-block;
    background-image: url(http://a.imimg.cn/r17259/images/array/connect_icon.png);
    background-repeat: no-repeat;
    padding-left: 40px;
    margin-top: 7px\9;
    cursor: pointer;
    line-height: 24px;
}

.platformusers li a.sina em {
    background-position: left -136px;
}

.platformusers li a.tencent em {
    background-position: left top;
}

.platformusers li a.renren em {
    background-position: left -34px;
}

.platformusers li a.qq em {
    background-position: left -68px;
}

.platformusers li a.douban em {
    background-position: left -198px;
}

.platformusers li a.taobao em {
    background-position: left -234px;
}

.newreg .hsplit,.newreg2 .hsplit {
    height: 72px;
    background: url(http://a.imimg.cn/r17259/images/reg/or.png) no-repeat center center;
}

.zmusers,.losepwd,.sendbox {
    width: 360px;
}

.zmusers h2,.losepwd h2,.sendbox h2 {
    text-align: center;
    font-size: 14px;
    padding-bottom: 24px;
}

.zmusers h2 .title,.losepwd h2 .title,.sendbox h2 .title {
    background: url(http://a.imimg.cn/r17259/images/reg/toright.gif) no-repeat right 3px;
    padding-right: 14px;
}

.zmusers h2 .unfold,.losepwd h2 .unfold,.sendbox h2 .unfold {
    background-position: right -47px;
}

.zmusers .regform,.losepwd .thisform,.sendbox .sendboxbody {
    padding-bottom: 10px;
}

.zmusers .regform dl,.losepwd .thisform dl {
    height: 56px;
    display: block;
}

.zmusers .regform dl.special,.losepwd .thisform dl.special {
    margin-bottom: 6px;
}

.zmusers .regform dl.dl-getpwd {
    margin-bottom: 10px;
    height: auto;
}

.zmusers .regform dt,.losepwd .thisform dt {
    float: left;
    width: 56px;
    padding-right: 6px;
    line-height: 28px;
    text-align: right;
    display: block;
}

.zmusers .regform dd,.losepwd .thisform dd {
    float: left;
    display: block;
}

.inputouter {
    _line-height: 1.5;
    _font-size: 12px;
}

.zmusers .regform dd .inputouter input,.losepwd .thisform dd .inputouter input {
    width: 260px;
}

.zmusers .regform dd .inputerror input,.losepwd .thisform dd .inputerror input,.zmusers .regform dd .inputouter input.errortest,.losepwd .thisform dd .inputouter input.errortest {
    width: 258px;
    height: 26px;
    padding-left: 4px;
    border: 2px solid #f86;
    background: #fec;
}

.zmusers .regform .innerborder {
    position: static;
}

.zmusers .regform dd .errortips {
    color: #f86;
}

.zmusers .regform .dl-getpwd dd {
    width: 266px;
}

.zmusers .regform .dl-getpwd label {
    vertical-align: middle;
    _vertical-align: bottom;
    padding-left: 4px;
}

.zmusers .regform .dl-getpwd input {
    _vertical-align: bottom;
}

.zmusers .regform .btn-login {
    background: url(http://a.imimg.cn/r17259/images/button/signup_btn.gif) no-repeat left top;
    display: block;
    width: 123px;
    height: 40px;
    overflow: hidden;
}

.losepwd .thisform .btn-getpwd {
    background: url(http://a.imimg.cn/r17259/images/signup/button.gif) no-repeat left top;
    display: block;
    width: 122px;
    height: 40px;
    overflow: hidden;
}

.losepwd .thisform .btn-getpwd:hover {
    background-position: -140px 0;
}

.losepwd .thisform .btn-ungetpwd,.losepwd .thisform .btn-ungetpwd:hover {
    background-position: -420px 0;
    cursor: default;
}

.sendbox .mailbox {
    background: #fffcce;
    padding: 12px 6px;
    width: 300px;
    margin: 0 auto 12px;
    text-align: center;
    line-height: 22px;
}

.sendbox .sendbutton {
    text-align: center;
}

.sendbox .btn-gray36 {
    margin-right: 10px;
}

.sendbox .btn-gray36 em {
    width: 82px;
}

.newreg2 h2 {
    height: 60px;
    padding-top: 10px;
    text-align: center;
}

.adduserinfo .avatar {
    float: left;
    width: 178px;
    text-align: center;
}

.adduserinfo .thisform {
    background: url(http://a.imimg.cn/r17259/images/chrome/line_vertical_bg.gif) no-repeat;
    float: left;
    padding-left: 20px;
}

.adduserinfo .thisform dl {
    clear: both;
    padding-bottom: 22px;
    overflow: hidden;
    display: block;
    zoom: 1;
}

.adduserinfo .thisform dt,.adduserinfo .thisform dd {
    float: left;
    display: block;
}

.adduserinfo .thisform dt {
    width: 60px;
    padding-right: 8px;
    text-align: right;
    font-size: 14px;
}

.adduserinfo .thisform dt.pad-t {
    padding-top: 8px;
}

.adduserinfo .thisform .inputtips {
    padding: 8px 0 0 6px;
    width: 130px;
    color: #888;
}

.adduserinfo .thisform .inputwarn {
    padding: 8px 0 0 6px;
    width: 130px;
    color: #f86;
}

.adduserinfo .thisform .inputouter input {
    width: 194px;
    height: 34px;
}

.adduserinfo .thisform label {
    padding: 0 24px 0 6px;
    vertical-align: middle;
    _vertical-align: bottom;
}

.adduserinfo .thisform .btn-register {
    background: url(http://a.imimg.cn/r17259/images/signup/reg_btn.gif) no-repeat;
    display: inline-block;
    width: 142px;
    height: 40px;
}
}
