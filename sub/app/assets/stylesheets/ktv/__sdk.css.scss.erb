      box-shadow: rgba(0, 0, 0, 0.6) 0 -1px 0, rgba(0, 0, 0, 0.6) -1px 0 0, rgba(0, 0, 0, 0.6) 1px 0 0, inset rgba(255, 255, 255, 0.2) 0 1px;
  }
  #tabs ul li:before, #tabs ul li:after {
      content: '';
      width: 7px;
      height: 6px;
      position: absolute;
      z-index: 2;
      bottom: 1px;
      background: #323232;
      background: #5e5b5c -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #373737),  color-stop(100%,  #323232));
      background: #5e5b5c -webkit-linear-gradient(#373737,  #323232);
