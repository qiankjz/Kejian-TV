  }
  header #account-notifications{
    width:35px;
    height:35px;
    float:left;
    background-color: #2a2a2a;
    background-image: -webkit-gradient(linear,  left top,  left bottom,  from(#2a2a2a),  to(#222));
    background-image: -webkit-linear-gradient(top,  #2a2a2a,  #222);
    background-image: -moz-linear-gradient(top,  #2a2a2a,  #222);
    background-image: -o-linear-gradient(top,  #2a2a2a,  #222);
    background-image: -ms-linear-gradient(top,  #2a2a2a,  #222);
    background-image: linear-gradient(top,  #2a2a2a, #222222);
    border: 1px solid #141414;
    -webkit-box-shadow: 0 1px 0px rgba(0, 0, 0, 0), inset 0 0 0 rgba(0, 0, 0, 0.9);
    -moz-box-shadow: 0 1px 0px rgba(0, 0, 0, 0), inset 0 0 0 rgba(0, 0, 0, 0.9);
    box-shadow: 0 1px 0px rgba(0, 0, 0, 0), inset 0 0 0 rgba(0, 0, 0, 0.9);
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    -ms-border-radius: 3px;
    -o-border-radius: 3px;
    border-radius: 3px;
    border-bottom: 1px solid #141414;    
    .count-badge {
      position: absolute;
      margin-left: 26px;
      top: -3px;
      z-index: 200;
      padding: 0 4px;
      height: 14px;
      line-height: 14px;
      border: 1px solid #672c03;
      -webkit-border-radius: 13px;
      -moz-border-radius: 13px;
      border-radius: 13px;
      background-color: #fca721;
      background-image: -moz-linear-gradient(top,#fca721,#f86d12);
      background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0,#fca721),color-stop(1,#f86d12));
      background-image: linear-gradient(to bottom,#fca721,#f86d12);
      filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#fca721',EndColorStr='#f86d12');
      -webkit-box-shadow: inset 0 1px 0 #ffcb29,inset 0 -1px 0 #ff7914,0px 1px 2px rgba(0,0,0,0.4);
      -moz-box-shadow: inset 0 1px 0 #ffcb29,inset 0 -1px 0 #ff7914,0px 1px 0 rgba(0,0,0,0.5);
      box-shadow: inset 0 1px 0 #ffcb29,inset 0 -1px 0 #ff7914,0px 1px 2px rgba(0,0,0,0.4);
      color: #fff;
      font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
      text-shadow: 0 1px 0 #8f3f06;
      text-align: center;
      text-decoration: none;
      font-size: 11px;
      font-weight: bold;
      -webkit-font-smoothing: antialiased;
      -moz-background-clip: padding-box;
      -webkit-background-clip: padding-box;
      background-clip: padding-box;
      cursor:pointer;
    }
    a{
      display:block;
      opacity:0.66;
      width:33px;
      height:28px;
      margin-left:2px;
      margin-top:6px;
      background: transparent url(<%= asset_path 'ring.png' %>) no-repeat 0 0;
      cursor:pointer;
      text-indent:9999px;
      overflow:hidden;
      &:hover{
        opacity:1;
        background-position:0 0;
      }
      &:active{
        opacity:1;
        background-position:0 -24px;
      }
    }
  }
