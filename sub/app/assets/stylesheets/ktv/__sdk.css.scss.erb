      z-index: 10;
  }
  #tabs ul li {
      float: left;
      margin: 0 12px 0 0;
      position: relative;
      padding: 0 0 1px;
  }
  #tabs ul a {
      display: block;
      color: #fff;
      text-decoration: none;
      padding: 0 15px;
      line-height: 29px;
      height: 29px;
      font-weight: bold;
      background: #323232;
      background: #5e5b5c -webkit-gradient(linear,  50% 0%,  50% 100%,  color-stop(0%,  #5e5b5c),  color-stop(100%,  #323232));
      background: #5e5b5c -webkit-linear-gradient(#5e5b5c,  #323232);
      background: #5e5b5c -moz-linear-gradient(#5e5b5c,  #323232);
      background: #5e5b5c -o-linear-gradient(#5e5b5c,  #323232);
      background: #5e5b5c -ms-linear-gradient(#5e5b5c,  #323232);
      background: #5e5b5c linear-gradient(#5e5b5c, #323232);
      border-bottom: none;
      -webkit-background-clip: padding-box;
      -webkit-font-smoothing: subpixel-antialiased;
      -webkit-border-radius: 6px 6px 0 0;
      -moz-border-radius: 6px 6px 0 0;
      -ms-border-radius: 6px 6px 0 0;
      -o-border-radius: 6px 6px 0 0;
      border-radius: 6px 6px 0 0;
      text-shadow: #000 0 -1px 0;
      position: relative;
      -webkit-box-shadow: rgba(0, 0, 0, 0.6) 0 -1px 0, rgba(0, 0, 0, 0.6) -1px 0 0, rgba(0, 0, 0, 0.6) 1px 0 0, inset rgba(255, 255, 255, 0.2) 0 1px;
