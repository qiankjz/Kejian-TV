.__old{
  a{
    text-decoration:none;
  }
  /*
  Kejian.TV
  Crafted by P.S.V.R
  Inspired by speakerdeck.com and selectivizr.com
  With IE 6,7,8 compatibility in mind
  */

  /*global class*/
  .abs{
    position: absolute;
  }
  .w960{
    width:960px;
    margin:0 auto;
  }
  .w460{
    width:460px;
    margin:0 auto;
  }
  .fl{
    float:left;
  }
  .fr{
    float:right;
  }
  .btn{
    display:block;
    text-decoration:none;  
    font-size:15px;
    height:48px;
    line-height:48px;
    color:white;
    text-align:center;
    outline:none;
  }
  .btn:hover{
    color:white;
    background-position:0 -48px ;
  }
  .btn:active{
    background-position:0 -96px;
  }
  .btn_going_in{
    cursor: default !important;
    background-position:0 -96px !important;
  }
  .btn_going_in:hover{
    background-position:0 -96px; 
  }
  .btn:focus { 
    outline: none;
  }
  .btn301{
    background:url(<%=asset_path 'ie/ktv_regbtn.png'%>);
    width:301px;  
  }
  .btn210{
    background:url(<%=asset_path 'ie/btn210.png'%>);
    width:210px;
  }
  .btn210_grey{
    background:url(<%=asset_path 'ie/btn210_grey.png'%>);
    width:210px;
  }
  /*global tags*/
  body{
    color: #999;
    background: #333 url(<%=asset_path('ie/grey.jpg')%>);
    -webkit-font-smoothing: antialiased;
    font: 15px "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, sans-serif;
    margin: 0;
    padding: 0;
    font-weight: 300;
  }
  input[type="text"], input[type="password"], textarea{
    color: #333333;
  }
  form.normal{
    input{
      border:none;
      padding-top:3px;
      height:41px;
      line-height:46px;
      text-indent:15px;
      font-size:18px;
      color: #333333;
      font-family:"HelveticaNeue-Light","Helvetica Neue Light","Helvetica Neue",Helvetica,Arial,sans-serif;
      background-repeat: no-repeat;
      background-color:transparent;
    }
    input.s462{
      background-image: url(<%=asset_path 'ie/input462.png'%>);
      width:462px;
    }
    input.s212{
      background-image: url(<%=asset_path 'ie/input212.png'%>);
      width:212px;
    }
    input:focus{
      background-position:0 -45px;
    }
    label{
      color:#555;
      display:block;
      float:left;
    }
    div.field_error{
      color:red;
      background-color:#FFCFCF;
      float:right;
      max-width:384px;
    }
    #regerror_password,#regerror_password_confirmation{
      max-width:167px;
    }

  }
  /*Sky.1  #header ------------------------------------------------*/
  #header0{
    background: #424242 url(<%=asset_path('ie/header0.png')%>) repeat-x;
    width:100%;
    height:40px;
    font-family:"HelveticaNeue","Helvetica Neue",Helvetica,Arial,sans-serif;
    div div{
      cursor:pointer;
    }
    a{
      line-height:40px;
      text-shadow:0 1px 0 rgba(0, 0, 0, 0.5);
      font-size:12px;
      font-weight:bold;
      color:#C2C2C2;
      margin-left:40px;
      &:hover{
        text-decoration:none;
      }
    }
    img{
      opacity: 0.7;
      position:absolute;
      margin-left:12px;
      margin-top:9px;
    }
    .active{
      img{
        display:none;
      }
      a{
        color:white;
      }
    }
    .profile{
      width:54px;
      height:40px;
      float:right;
    }
    .spliter{
      background: transparent url(<%= asset_path('ie/header0spliter.png') %>) no-repeat;
      width:2px;
      height:40px;
      float:right;
    }
    #header0btn{
      float:right;
      background: #4A9CD6 url(<%= asset_path('ie/header0btn.png') %>) no-repeat;
      width:40px;
      height:30px;
      margin:4px 0 0 11px;
      &:hover{
        background-position:0 -30px;
      }
      &:active{
        background-position:0 -60px;
      }
    }
    #slab1{
      width:129px;
      height:40px;
      float:left;
    }
    #slab1:hover img, #slab2:hover img, #slab3:hover img, #slab4:hover img, #slab_profile:hover img{
      opacity: 1;
    }
    #slab1:hover a, #slab2:hover a, #slab3:hover a, #slab4:hover a, #slab_profile:hover a{
      color:#FFF;
    }
    #slab1.active{
      background: #222 url(<%= asset_path('ie/header0_1active.png') %>) no-repeat;
    }
    #slab2{
      width:111px;
      height:40px;
      float:left;
    }
    #slab2.active{
      background: #222 url(<%= asset_path('ie/header0_2active.png') %>) no-repeat;
    }
    #slab3{
      width:83px;
      height:40px;
      float:left;
      img{
        margin-top:10px;
      }
      a{
        margin-left:37px;
      }
    }
    #slab3.active{
      background: #222 url(<%= asset_path('ie/header0_3active.png') %>) no-repeat;
    }

  }
  #header{
    background: #1E1E1E url(<%=asset_path("ie/dark.jpg")%>) repeat-x;
    height: 130px;
    border-bottom:1px solid #2d2d2d;
  }
    /*Sky.1.1 #headnav ------------------------------------------*/
    #headnav{
      height:50px;
      color:#C6C6C6;
      width:100%;
      background: #424242;
    	border-bottom:1px solid #222;
    }
    #headnav a{
    	background: url(<%=asset_path("ie/sprites2.png")%>) 0 0 no-repeat;
    	display:block;
    	padding: 8px 0 3px 45px;
    	color:#fff;
    	text-decoration: none;
    	font-size:12px;
    	opacity: 0.7;
    	text-shadow: 0 1px 2px #000;
    	-webkit-transition: 0.25s ;
    	-moz-transition: 0.25s;
    }
    #headnav a:hover{
    	opacity: 1;    
    }
    #headnav em {
    	font-style:normal;
    	font-weight: bold;
    	display:block;
    	font-size: 14px;
    }
    #headnav_content div{
      position: absolute;
      text-align:center;
    }
    #headnav div.first{width:140px;margin-left: 0;}
    #headnav div.second{width:143px;margin-left: 260px;}
    #headnav div.third{width:129px;margin-left: 546px;}
    #headnav div.fourth{width:144px;margin-left: 816px;}

    #headnav div.first a{background-position: -520px 0;}
    #headnav div.second a{background-position: -1040px 0;}
    #headnav div.third a{background-position: -1298px 0;}
    #headnav div.fourth a{background-position: -780px 0;}
    #headnav div.first_selected a{background-position: -520px -44px;}
    #headnav div.second_selected a{background-position: -1040px -44px;}
    #headnav div.third_selected a{background-position: -1298px -44px;}
    #headnav div.fourth_selected a{background-position: -780px -44px;}

    #headnav_content div.selected a{
      color: #FFDD33;
      opacity: 1;
    }

    /*Sky.1.2 #logodiv ------------------------------------------*/
    /*logoabsdiv*/
    #logoabsdiv{
      margin-top:25px;
    }
      #logolink{
        position: absolute;
        color:white;
        margin-top:1px;
      }
      #logotext{
        padding:8px 6px;
      }
      #searchbox {
        position: absolute;
        background: url(<%=asset_path("ie/searchbox.png")%>) no-repeat;
        width:250px;height:47px;
        margin: -4px 0 0 188px;
      }
      #searchbox.focused input{color:#FFF;}
      #searchbox.focused{
        background-position: 0 -47px;
      }
      #searchbox.focused input{color:#333333;}
      #searchinput {
        position: absolute;
      	border: 0;
        color: #999999;
        background:transparent;
        font-size: 13px;
        height: 20px;
        left:38px;top:13px;
        width: 194px;
        -webkit-transition: background, color 0.2s linear;
        -moz-transition: background, color 0.2s linear;
        transition: background, color 0.2s linear;
        -webkit-font-smoothing: antialiased;
      }
      #loginbox{
        position: absolute;
        margin-bottom: -6px;
        margin:8px 0 0 832px;
      }
      #loginbox a{
        color:#E1E1E1;
        font-size:16px;
        text-decoration:none;
      }
      #loginbox a:hover{
        color:#FFF;
      }
      #login{}
      #register{margin-left:10px;}

  /*SkyLine.2  #skyline ------------------------------------------------*/
  #skyline{
    padding:25px 10px;
    color:#88D2AD;
    background:url(<%=asset_path("ie/grey.jpg")%>);
  }
  /*3. Ground*/
  #main{
    background: #E4E4E4 url(<%=asset_path('ie/white.png')%>);
  }
  div.acquired {
    text-align:center;
    background:#000;
    font-size:12px;
    height:32px;
    line-height:32px;
    -webkit-font-smoothing:subpixel-antialiased;
  }

  /*-1. utilities */
  p.flash_notice, p.flash_alert, p.flash_error {
    background: #522855;
    -webkit-box-shadow: inset rgba(255, 255, 255, 0.08) 0 1px 0, inset rgba(0, 0, 0, 0.5) 0 -1px 0;
    -moz-box-shadow: inset rgba(255, 255, 255, 0.08) 0 1px 0, inset rgba(0, 0, 0, 0.5) 0 -1px 0;
    box-shadow: inset rgba(255, 255, 255, 0.08) 0 1px 0, inset rgba(0, 0, 0, 0.5) 0 -1px 0;
    color: #fff;
    font-size: 15px;
    font-weight: 700;
    height: 38px;
    line-height: 39px;
    margin: 0;
    text-align: center;
    text-shadow: rgba(0, 0, 0, 0.8) 0 -1px 0;
  }

  
}