/* scss/modules/post_it.css */

.post-it {
    border: 1px solid #E5D387;
    background: #FFFCD8;
    background: -moz-linear-gradient(113deg, #e5d387, #fffcd8 28%);
    background: -webkit-gradient(linear, 100% 100%, 85% 53%, from(#e5d387), to(#fffcd8));
    background: -o-linear-gradient(113deg, #e5d387, #fffcd8 28%);
    background: linear-gradient(113deg, #e5d387,#fffcd8 28%);
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    border-radius: 6px;
    position: relative;
    zoom: 1;
}

.post-it .wrapper {
    border: 1px solid #FFF;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    padding: 0 !important;
}

.post-it .content {
    padding: 15px 20px 15px 15px;
}

.post-it .content .attn {
    color: #000;
    font-size: 28px;
    line-height: 1;
    margin: 0 0 10px;
}

.en .post-it .content .attn {
    font-family: 'LazyDogRegular', Arial, sans-serif;
}

.ja .post-it .content .attn,.ko .post-it .content .attn {
    font-family: sans-serif;
}

.post-it .content p {
    font-size: 13px;
    color: #666;
    line-height: 1.5;
    margin: 5px 0;
}

.post-it .content p:first-child {
    margin: 0;
}

.post-it.float {
    -moz-box-shadow: 1px 1px 10px #cfcfcf;
    -webkit-box-shadow: 1px 1px 10px #cfcfcf;
    box-shadow: 1px 1px 10px #cfcfcf;
}

.curl,.curl:after,.curl .wrapper {
    -webkit-border-bottom-right-radius: 150px 25px;
    -moz-border-radius-bottomright: 150px 25px;
    border-bottom-right-radius: 150px 25px;
}

.curl:before {
    background: transparent;
    bottom: 12px;
    -moz-box-shadow: 11px 11px 11px rgba(207,207,207,0.7);
    -webkit-box-shadow: 11px 11px 11px rgba(207,207,207,0.7);
    box-shadow: 11px 11px 11px rgba(207,207,207,0.7);
    content: '';
    height: 50%;
    position: absolute;
    right: 12px;
    -moz-transform: rotate(3deg) skew(5deg);
    -webkit-transform: rotate(3deg) skew(5deg);
    transform: rotate(3deg) skew(5deg);
    width: 50%;
    z-index: -1;
}

.curl:after {
    background: transparent;
    bottom: 13px;
    -moz-box-shadow: 8px 12px 10px rgba(0,0,0,0.25);
    -webkit-box-shadow: 8px 12px 10px rgba(0,0,0,0.25);
    box-shadow: 8px 12px 10px rgba(0,0,0,0.25);
    content: '';
    height: 70%;
    opacity: 50%;
    position: absolute;
    right: 11px;
    -moz-transform: rotate(5deg) skew(10deg);
    -webkit-transform: rotate(5deg) skew(10deg);
    transform: rotate(5deg) skew(10deg);
    width: 70%;
    z-index: -2;
